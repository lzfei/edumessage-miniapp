.star {
  width: 10px;
  height: 10px;
}
.score {
  padding-left: 5px;
  color: #FF9800;
  font-size: 12px;
}
.school-base {
  margin-top: 10px;
  border-bottom: 1px solid #E5E5E5;
  padding: 5px;
}
.school-base view{
  display: inline-block;
  width: 33.333%;
  text-align: center;
  font-size: 12px;
  color: #AFAFAF;
}
.school-base .base-score {
  width: 32%;
}
.school-base .base-info {
  border-right: 1px solid #E5E5E5;
}
.school-base view text {
  display: block;
  font-size: 20px;
  color: #101010;
}
.ident-block {
  display:inline-block;
}
.ident {
  font-size: 12px;
  display: flex;
}
.ident image {
  width: 20px;
  height: 20px;
  margin-top: 10px;
  padding: 0 10px;
}
.ident text {
  vertical-align:middle;
  line-height: 40px;
}
.good-at {
  padding: 10px 15px;
}
.good-at text {
  margin-right: 10px;
  padding: 2px 15px;
  font-size: 14px;
  border: 1px solid #28B8A1;
  border-radius: 15px;
}
.weui-article__p {
  padding: 15px;
  font-size: 14px;
}
.weui-cells {
  font-size: 14px;
}